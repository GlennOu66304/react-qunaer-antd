vel/src/components/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/header/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/carousel/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/productCollection/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/businessPartners/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/footer/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/sideMenu/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/footer/Footer.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/sideMenu/SideMenu.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/businessPartners/BusinessPartners.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/productCollection/ProducCollection.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/carousel/Carousel.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/header/Header.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/productCollection/ProductImage.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/sideMenu/mockup.ts",[],"/Users/glenn/Desktop/react-travel/src/mockups.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/home/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/home/HomePage.tsx",[],"/Users/glenn/Desktop/react-travel/src/pages/home/mockups.ts",[],["217","218"],"/Users/glenn/Desktop/react-travel/src/pages/detail/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/detail/DetailPage.tsx",["219"],"import React from \"react\";\nimport styles from \"./DetailPage.module.css\";\n// 3)RouteComponentProps react-rotuer-dom\nimport { RouteComponentProps, useParams } from \"react-router-dom\";\nimport { Header, Footer, ProductIntro, ProductComment } from \"../../components\";\n// import axios from \"axios\";\nimport { useEffect } from \"react\";\nimport { Spin, Row, Col, DatePicker, Divider, Typography } from \"antd\";\nimport { commentMockData } from \"./mockup\";\n// import the actions\nimport { giveMeProductDetails } from \"../../reduex/productDetail/slice\";\n// impor the hooks\nimport { useSelector } from \"../../reduex/hooks\";\nimport { useDispatch } from \"react-redux\";\n// 4)interface prop types check\ninterface MatchParams {\n  touristRouteId: string;\n  comment: any;\n}\n\nexport const DetailPage: React.FC<RouteComponentProps<MatchParams>> = (\n  props\n) => {\n  // console.log(props)\n  // aftert add the  MatchParams and the <RouteComponentProps< MatchParams>> , the error after props.location will lose\n\n  // every router compoponetn will have props :2)match.history,location(props)\n  // console.log(props.history)\n  // console.log(props.location)\n  // console.log(props.match)\n\n  const { touristRouteId } = useParams<MatchParams>();\n  // loading, errors, productlist\n  const loading = useSelector((state) => state.productDetail.loading); // like this.state in the class based component\n  const error = useSelector((state) => state.productDetail.error);\n  const product = useSelector((state) => state.productDetail.data);\n  const dispatch = useDispatch();\n  const { RangePicker } = DatePicker;\n  useEffect(() => {\n    //same like componentdidmout in class component\n    //   fetch data function\n    const fetchData = async () => {\n     dispatch(giveMeProductDetails(touristRouteId))\n    };\n    // call back the function\n    fetchData();\n    // return something\n  }, []);\n\n  //   checked the outputed value\n  // console.log(product)\n\n  if (loading) {\n    return (\n      <Spin\n        size=\"large\"\n        style={{\n          marginTop: 200,\n          marginBottom: 200,\n          marginLeft: \"auto\",\n          marginRight: \"auto\",\n          width: \"100%\",\n        }}\n      />\n    );\n  }\n\n  if (error) {\n    return <div>网站出现错误:{error}</div>;\n  }\n\n  return (\n    <>\n      <Header />\n\n      {/* page conetent */}\n      <div className={styles[\"page-content\"]}>\n        {/* 1.product details and date */}\n        <div className={styles[\"product-intro-container\"]}>\n          <Row>\n            {/* left side: Produt Detail */}\n            <Col span={11}>\n              <ProductIntro\n                title={product.title}\n                shortDescription={product.description}\n                price={product.price}\n                coupons={product.coupons}\n                points={product.points}\n                discount={product.discount}\n                rating={product.rating}\n                picture={product.touristRoutePictures.map((p) => p.url)}\n              />\n            </Col>\n            {/* Right side:date */}\n            <Col span={13}>\n              <RangePicker open style={{ marginTop: 20 }} />\n            </Col>\n          </Row>\n        </div>\n        {/* 2. 锚点菜单*/}\n        <div className={styles[\"product-detail-anchor\"]}> </div>\n        {/* 3.产品特色 */}\n        <div id=\"feature\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>产品特色</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.features }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 4.费用 */}\n        <div id=\"fees\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>费用</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.fees }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 5预定须知 */}\n        <div id=\"notes\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>预定须知</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.notes }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 6.用户评价 */}\n        <div id=\"components\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>用户评价</Typography.Title>\n          </Divider>\n\n          <div style={{ margin: 40 }}>\n            <ProductComment data={commentMockData} />\n          </div>\n        </div>\n      </div>\n\n      <Footer />\n    </>\n  );\n};\n","/Users/glenn/Desktop/react-travel/src/pages/login/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/login/SignIn.tsx",[],"/Users/glenn/Desktop/react-travel/src/pages/register/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/register/SignUp.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/header/Header.Class.tsx",[],"/Users/glenn/Desktop/react-travel/src/reduex/store.ts",[],"/Users/glenn/Desktop/react-travel/src/i18n/configs.ts",[],"/Users/glenn/Desktop/react-travel/src/reduex/language/languageReducer.ts",["220"],"import i18n from 'i18next'\n\n// 1.import the actioncreator file;\nimport { CHANGE_LANGUAGE, ADD_LANGUAGE, LanguageActionTypes } from './languageActions';\n\n// 2.interface option\n\nexport interface LanguageState {\n  language: \"en\" | \"zh\";\n  languageList: { name: string; code: string }[];\n}\n\n//2.1 sett the defaule state value:// it is a obeject\n\n// language\n\n// Code\n\nconst defaultState: LanguageState = {\n  language: \"zh\",\n  languageList: [\n    { name: \"中文\", code: \"zh\" },\n    { name: \"English\", code: \"en\" },\n  ],\n};\n\n// How can I get rid of the warning import/no-anonymous-default-export in React?\n// https://stackoverflow.com/questions/64729264/how-can-i-get-rid-of-the-warning-import-no-anonymous-default-export-in-react\n// 3.axport the reducer\nexport default (state = defaultState, action:LanguageActionTypes) => {\n  // console.log(state,action);\n  // switch is a fuction\n  