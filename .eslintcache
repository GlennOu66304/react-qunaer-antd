/sideMenu/mockup.ts",[],"/Users/glenn/Desktop/react-travel/src/mockups.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/home/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/home/HomePage.tsx",[],"/Users/glenn/Desktop/react-travel/src/pages/home/mockups.ts",[],["177","178"],"/Users/glenn/Desktop/react-travel/src/pages/detail/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/detail/DetailPage.tsx",["179"],"import React from \"react\";\nimport styles from \"./DetailPage.module.css\";\n// 3)RouteComponentProps react-rotuer-dom\nimport { RouteComponentProps, useParams } from \"react-router-dom\";\nimport { Header, Footer, ProductIntro, ProductComment } from \"../../components\";\nimport axios from \"axios\";\nimport { useState, useEffect } from \"react\";\nimport { Spin, Row, Col, DatePicker, Divider, Typography } from \"antd\";\nimport { commentMockData } from \"./mockup\";\n// 4)interface prop types check\ninterface MatchParams {\n  touristRouteId: string;\n  comment: any;\n}\n\nexport const DetailPage: React.FC<RouteComponentProps<MatchParams>> = (\n  props\n) => {\n  // console.log(props)\n  // aftert add the  MatchParams and the <RouteComponentProps< MatchParams>> , the error after props.location will lose\n\n  // every router compoponetn will have props :2)match.history,location(props)\n  // console.log(props.history)\n  // console.log(props.location)\n  // console.log(props.match)\n\n  const { touristRouteId } = useParams<MatchParams>();\n  // loading, errors, productlist\n  const [loading, setLoading] = useState<boolean>(true); // like this.state in the class based component\n  const [error, setError] = useState<string | null>(null);\n  const [product, setProduct] = useState<any>(null);\n  const { RangePicker } = DatePicker;\n  useEffect(() => {\n    //same like componentdidmout in class component\n    //   fetch data function\n    const fetchData = async () => {\n      setLoading(true);\n      try {\n        const { data } = await axios.get(\n          `http://123.56.149.216:8089/api/touristRoutes/${touristRouteId}`\n        );\n        setProduct(data);\n        setLoading(false);\n        // console.log(data)\n      } catch (error: any) {\n        setError(error.message);\n      }\n    };\n    // call back the function\n    fetchData();\n    // return something\n  }, []);\n\n  //   checked the outputed value\n  // console.log(product)\n\n  if (loading) {\n    return (\n      <Spin\n        size=\"large\"\n        style={{\n          marginTop: 200,\n          marginBottom: 200,\n          marginLeft: \"auto\",\n          marginRight: \"auto\",\n          width: \"100%\",\n        }}\n      />\n    );\n  }\n\n  if (error) {\n    return <div>网站出现错误:{error}</div>;\n  }\n\n  return (\n    <>\n      <Header />\n\n      {/* page conetent */}\n      <div className={styles[\"page-content\"]}>\n        {/* 1.product details and date */}\n        <div className={styles[\"product-intro-container\"]}>\n          <Row>\n            {/* left side: Produt Detail */}\n            <Col span={11}>\n              <ProductIntro\n                title={product.title}\n                shortDescription={product.description}\n                price={product.price}\n                coupons={product.coupons}\n                points={product.points}\n                discount={product.discount}\n                rating={product.rating}\n                picture={product.touristRoutePictures.map((p) => p.url)}\n              />\n            </Col>\n            {/* Right side:date */}\n            <Col span={13}>\n              <RangePicker open style={{ marginTop: 20 }} />\n            </Col>\n          </Row>\n        </div>\n        {/* 2. 锚点菜单*/}\n        <div className={styles[\"product-detail-anchor\"]}> </div>\n        {/* 3.产品特色 */}\n        <div id=\"feature\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>产品特色</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.features }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 4.费用 */}\n        <div id=\"fees\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>费用</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.fees }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 5预定须知 */}\n        <div id=\"notes\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>预定须知</Typography.Title>\n          </Divider>\n\n          <div\n            dangerouslySetInnerHTML={{ __html: product.notes }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n        {/* 6.用户评价 */}\n        <div id=\"components\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={1}>用户评价</Typography.Title>\n          </Divider>\n\n          <div style={{ margin: 40 }}>\n            <ProductComment data={commentMockData}/>\n          </div>\n        </div>\n      </div>\n\n      <Footer />\n    </>\n  );\n};\n","/Users/glenn/Desktop/react-travel/src/pages/login/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/login/SignIn.tsx",[],"/Users/glenn/Desktop/react-travel/src/pages/register/index.ts",[],"/Users/glenn/Desktop/react-travel/src/pages/register/SignUp.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/header/Header.Class.tsx",[],"/Users/glenn/Desktop/react-travel/src/reduex/store.ts",[],"/Users/glenn/Desktop/react-travel/src/i18n/configs.ts",[],"/Users/glenn/Desktop/react-travel/src/reduex/language/languageReducer.ts",["180"],"import i18n from 'i18next'\n\n// 1.import the actioncreator file;\nimport { CHANGE_LANGUAGE, ADD_LANGUAGE, LanguageActionTypes } from './languageActions';\n\n// 2.interface option\n\nexport interface LanguageState {\n  language: \"en\" | \"zh\";\n  languageList: { name: string; code: string }[];\n}\n\n//2.1 sett the defaule state value:// it is a obeject\n\n// language\n\n// Code\n\nconst defaultState: LanguageState = {\n  language: \"zh\",\n  languageList: [\n    { name: \"中文\", code: \"zh\" },\n    { name: \"English\", code: \"en\" },\n  ],\n};\n\n// How can I get rid of the warning import/no-anonymous-default-export in React?\n// https://stackoverflow.com/questions/64729264/how-can-i-get-rid-of-the-warning-import-no-anonymous-default-export-in-react\n// 3.axport the reducer\nexport default (state = defaultState, action:LanguageActionTypes) => {\n  // console.log(state,action);\n  // switch is a fuction\n  switch (action.type) {\n    // 2)change the case type\n    case CHANGE_LANGUAGE: {\n      i18n.changeLanguage(action.payload);//3.reducer file update: language api use\n      // 6.copay the previous state, then add the action payload\n      return { ...state, language: action.payload };\n    }\n    case  ADD_LANGUAGE:{//3.redeucer update\n\n         return { ...state, languageList: [...state.languageList, action.payload]};//copy the original state and state.languageList as well\n    }\n    default:\n      return state;\n  }\n};\n","/Users/glenn/Desktop/react-travel/src/reduex/language/languageActions.ts",[],"/Users/glenn/Desktop/react-travel/src/reduex/hooks.ts",[],"/Users/glenn/Desktop/react-travel/src/reduex/recommendation/recommendationReducer.ts",["181"],"//4. import the action type first\nimport {\n  FETCH_RECOMMEND_PRODUCTS_START,\n  FETCH_RECOMMEND_PRODUCTS_SUCCESS,\n  FETCH_RECOMMEND_PRODUCTS_FAIL,\n  RecommendProductActionTypes,\n} from \"./recommendationActions\";\n\n// 1.interface type check\nexport interface recommendationState {\n  productList: any[];\n  loading: boolean;\n  error: string | null;\n}\n// 2. default state check\n\nconst defaultState: recommendationState = {\n  productList: [],\n  loading: true,\n  error: null,\n};\n\n//3. return this reducer\nexport default (state = defaultState, action: RecommendProductActionTypes) => {\n  switch (action.type) {\n    case FETCH_RECOMMEND_PRODUCTS_START: {\n      return { ...state, loading: true };\n    }\n\n    case FETCH_RECOMMEND_PRODUCTS_SUCCESS: {\n      return { ...state, loading: false, productList: action.payload };\n    }\n    case FETCH_RECOMMEND_PRODUCTS_FAIL: {\n      return { ...state, loading: false, error: action.payload };\n    }\n    default:\n      return state;\n  }\n};\n","/Users/glenn/Desktop/react-travel/src/reduex/recommendation/recommendationActions.ts",[],"/Users/glenn/Desktop/react-travel/src/reduex/middleware/actionlog.ts",[],"/Users/glenn/Desktop/react-travel/src/components/productIntro/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/productIntro/ProductIntro1.tsx",[],"/Users/glenn/Desktop/react-travel/src/components/productComment/index.ts",[],"/Users/glenn/Desktop/react-travel/src/components/productComment/ProductComment.tsx",["182"],"/Users/glenn/Desktop/react-travel/src/pages/detail/mockup.ts",[],{"ruleId":"183","replacedBy":"184"},{"ruleId":"185","replacedBy":"186"},{"ruleId":"183","replacedBy":"187"},{"ruleId":"185","replacedBy":"188"},{"ruleId":"189","severity":1,"message":"190","line":52,"column":6,"nodeType":"191","endLine":52,"endColumn":8,"suggestions":"192"},{"ruleId":"193","severity":1,"message":"194","line":30,"column":1,"nodeType":"195","endLine":47,"endColumn":3},{"ruleId":"193","severity":1,"message":"194","line":24,"column":1,"nodeType":"195","endLine":39,"endColumn":3},{"ruleId":"196","severity":1,"message":"197","line":2,"column":25,"nodeType":"198","messageId":"199","endLine":2,"endColumn":32},"no-native-reassign",["200"],"no-negated-in-lhs",["201"],["200"],["201"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'touristRouteId'. Either include it or remove the dependency array.","ArrayExpression",["202"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","@typescript-eslint/no-unused-vars","'Tooltip' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation",{"desc":"203","fix":"204"},"Update the dependencies array to be: [touristRouteId]",{"range":"205","text":"206"},[1839,1841],"[touristRouteId]"]